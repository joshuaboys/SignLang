{
    "$schema": "https://raw.githubusercontent.com/Azure/BatchAI/master/schemas/2018-05-01/job.json",
    "properties": {
        "nodeCount": 1,
        "tensorFlowSettings": {
            "pythonScriptFilePath": "$AZ_BATCHAI_JOB_MOUNT_ROOT/scripts/master.py",
            "masterCommandLineArgs": "$AZ_BATCHAI_JOB_MOUNT_ROOT/video $AZ_BATCHAI_JOB_MOUNT_ROOT/scripts/model",
            "workerCommandLineArgs": "$AZ_BATCHAI_JOB_MOUNT_ROOT/video $AZ_BATCHAI_JOB_MOUNT_ROOT/scripts/model",
            "parameterServerCommandLineArgs": "$AZ_BATCHAI_JOB_MOUNT_ROOT/video $AZ_BATCHAI_JOB_MOUNT_ROOT/scripts/model"
        },
        "stdOutErrPathPrefix": "$AZ_BATCHAI_JOB_MOUNT_ROOT/scripts",
        "mountVolumes": {
            "azureFileShares": [
                {
                    "azureFileUrl": "https://cognitivecharades001.file.core.windows.net/videos",
                    "relativeMountPath": "videos"
                },
                {
                    "azureFileUrl": "https://cognitivecharades001.file.core.windows.net/scripts",
                    "relativeMountPath": "scripts"
                }
            ]
        },
        "jobPreparation": {
          "commandLine": "pip install keras tqdm"
        },
        "containerSettings": {
            "imageSourceRegistry": {
                "image": "tensorflow/tensorflow:1.8.0-gpu"
            }
        }
    }
}